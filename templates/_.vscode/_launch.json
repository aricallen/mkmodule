{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "sketch",
      "program": "${workspaceFolder}/local/sketch.js",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "test",
      "program": "${workspaceFolder}/node_modules/tape/bin/tape",
      "args": [
        "tests/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server",
      "program": "${workspaceFolder}/server.js",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "index",
      "program": "${workspaceFolder}/index.js",
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    }
  ]
}